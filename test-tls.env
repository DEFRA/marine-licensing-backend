# Environment variables for TLS testing
# Copy this file to .env and modify as needed, then run:
# node -r dotenv/config test-oidc-connection.js

# Target URL for testing
OIDC_URL=https://cdp-defra-id-stub.test.cdp-int.defra.cloud/cdp-defra-id-stub/.well-known/openid-configuration

# Environment
CDP_ENVIRONMENT=test

# Proxy settings (if needed)
# HTTP_PROXY=http://your-proxy:port
# HTTPS_PROXY=http://your-proxy:port
# NO_PROXY=localhost,127.0.0.1

# TLS/SSL settings
# Set to 0 to disable certificate validation (ONLY FOR TESTING)
# NODE_TLS_REJECT_UNAUTHORIZED=0

# Enable secure context for custom certificates
ENABLE_SECURE_CONTEXT=true

# Custom certificates (PEM format)
# TRUSTSTORE_1="-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----"
# TRUSTSTORE_2="-----BEGIN CERTIFICATE-----\n...\n-----END CERTIFICATE-----" 